{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "swiprolog",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "directive"
          },
          {
            "type": "SYMBOL",
            "name": "clause"
          }
        ]
      }
    },
    "directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ":-"
        },
        {
          "type": "SYMBOL",
          "name": "_term"
        },
        {
          "type": "STRING",
          "value": "."
        }
      ]
    },
    "clause": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_term"
        },
        {
          "type": "STRING",
          "value": "."
        }
      ]
    },
    "_term": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_restricted_operators_term"
        }
      ]
    },
    "_restricted_operators_term": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "compound_term"
        },
        {
          "type": "SYMBOL",
          "name": "atom"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "variable"
        },
        {
          "type": "SYMBOL",
          "name": "list_literal"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "PREC",
          "value": 5,
          "content": {
            "type": "SYMBOL",
            "name": "operator_term"
          }
        },
        {
          "type": "SYMBOL",
          "name": "parenthesized_term"
        },
        {
          "type": "SYMBOL",
          "name": "curly_braced_term"
        }
      ]
    },
    "compound_term": {
      "type": "PREC",
      "value": 100,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "functor",
            "content": {
              "type": "SYMBOL",
              "name": "atom"
            }
          },
          {
            "type": "STRING",
            "value": "("
          },
          {
            "type": "PREC",
            "value": 5,
            "content": {
              "type": "SYMBOL",
              "name": "_restricted_operators_term"
            }
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": ","
                },
                {
                  "type": "PREC",
                  "value": 5,
                  "content": {
                    "type": "SYMBOL",
                    "name": "_restricted_operators_term"
                  }
                }
              ]
            }
          },
          {
            "type": "STRING",
            "value": ")"
          }
        ]
      }
    },
    "atom": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "unquoted_atom"
        },
        {
          "type": "SYMBOL",
          "name": "quoted_atom"
        },
        {
          "type": "SYMBOL",
          "name": "graphic_char_atom"
        }
      ]
    },
    "unquoted_atom": {
      "type": "PATTERN",
      "value": "[a-z][a-zA-Z_]*"
    },
    "quoted_atom": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "'"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_single_quoted_inner"
          }
        },
        {
          "type": "PATTERN",
          "value": "'"
        }
      ]
    },
    "_single_quoted_inner": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "single_quoted_character_escape"
        },
        {
          "type": "SYMBOL",
          "name": "format_string_placeholder"
        },
        {
          "type": "SYMBOL",
          "name": "single_quoted_content"
        }
      ]
    },
    "format_string_placeholder": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "~"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "(`.)?t"
              },
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "PATTERN",
                        "value": "\\d+|\\*"
                      },
                      {
                        "type": "BLANK"
                      }
                    ]
                  },
                  {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ":"
                      },
                      {
                        "type": "BLANK"
                      }
                    ]
                  },
                  {
                    "type": "PATTERN",
                    "value": "[acd~DeEgfGiknNpqrR@s\\|\\+wW]"
                  }
                ]
              }
            ]
          }
        ]
      }
    },
    "single_quoted_character_escape": {
      "type": "PATTERN",
      "value": "\\\\[nrtv\\\\']"
    },
    "single_quoted_content": {
      "type": "PATTERN",
      "value": "[^\\\\~']+"
    },
    "graphic_char_atom": {
      "type": "PATTERN",
      "value": "!|[-+*/\\^<>=~:?@#$&][-+*/\\^<>=~:.?@#$&]*"
    },
    "string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "double_quoted_string"
        },
        {
          "type": "SYMBOL",
          "name": "backtick_string"
        }
      ]
    },
    "double_quoted_string": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\""
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_double_quoted_string_inner"
          }
        },
        {
          "type": "PATTERN",
          "value": "\""
        }
      ]
    },
    "_double_quoted_string_inner": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "format_string_placeholder"
        },
        {
          "type": "SYMBOL",
          "name": "double_quoted_character_escape"
        },
        {
          "type": "SYMBOL",
          "name": "double_quoted_content"
        }
      ]
    },
    "double_quoted_character_escape": {
      "type": "PATTERN",
      "value": "\\\\[nrtv\\\\\"]"
    },
    "double_quoted_content": {
      "type": "PATTERN",
      "value": "[^\\\\~\"]+"
    },
    "backtick_string": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "`"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_backtick_string_inner"
          }
        },
        {
          "type": "PATTERN",
          "value": "`"
        }
      ]
    },
    "_backtick_string_inner": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "format_string_placeholder"
        },
        {
          "type": "SYMBOL",
          "name": "backticked_character_escape"
        },
        {
          "type": "SYMBOL",
          "name": "backticked_content"
        }
      ]
    },
    "backticked_character_escape": {
      "type": "PATTERN",
      "value": "\\\\[nrtv\\\\`]"
    },
    "backticked_content": {
      "type": "PATTERN",
      "value": "[^\\\\~`]+"
    },
    "variable": {
      "type": "PATTERN",
      "value": "[_A-Z][a-zA-Z0-9_]*"
    },
    "list_literal": {
      "type": "PREC",
      "value": 100,
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "["
              },
              {
                "type": "STRING",
                "value": "]"
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "["
              },
              {
                "type": "PREC",
                "value": 15,
                "content": {
                  "type": "SYMBOL",
                  "name": "_restricted_operators_term"
                }
              },
              {
                "type": "PREC_LEFT",
                "value": 50,
                "content": {
                  "type": "REPEAT",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ","
                      },
                      {
                        "type": "PREC",
                        "value": 15,
                        "content": {
                          "type": "SYMBOL",
                          "name": "_restricted_operators_term"
                        }
                      }
                    ]
                  }
                }
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": "|"
                      },
                      {
                        "type": "SYMBOL",
                        "name": "_term"
                      }
                    ]
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              },
              {
                "type": "STRING",
                "value": "]"
              }
            ]
          }
        ]
      }
    },
    "number": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "0[xX][0-9a-fA-F]+"
        },
        {
          "type": "PATTERN",
          "value": "0b[01]+"
        },
        {
          "type": "PATTERN",
          "value": "0o[0-7]+"
        },
        {
          "type": "PATTERN",
          "value": "[-+]?[1-9][0-9]*"
        },
        {
          "type": "PATTERN",
          "value": "[-+]?0"
        }
      ]
    },
    "operator_term": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "left",
            "content": {
              "type": "SYMBOL",
              "name": "_term"
            }
          },
          {
            "type": "FIELD",
            "name": "operator",
            "content": {
              "type": "SYMBOL",
              "name": "operator"
            }
          },
          {
            "type": "FIELD",
            "name": "right",
            "content": {
              "type": "SYMBOL",
              "name": "_term"
            }
          }
        ]
      }
    },
    "operator": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_non_comma_operator"
        },
        {
          "type": "STRING",
          "value": ","
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "_non_comma_operator": {
      "type": "PATTERN",
      "value": "[-+*/\\^<>=~:.?@#$&]+"
    },
    "parenthesized_term": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "SYMBOL",
          "name": "_term"
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "curly_braced_term": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "SYMBOL",
          "name": "_term"
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}