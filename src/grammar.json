{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "swiprolog",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "read_term"
      }
    },
    "read_term": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_term"
        },
        {
          "type": "SYMBOL",
          "name": "read_term_end_token"
        }
      ]
    },
    "_term": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_restricted_operators_term"
        }
      ]
    },
    "_restricted_operators_term": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "compound_term"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "variable"
        },
        {
          "type": "SYMBOL",
          "name": "list_literal"
        },
        {
          "type": "SYMBOL",
          "name": "prefix_operator_term"
        },
        {
          "type": "PREC",
          "value": 1,
          "content": {
            "type": "SYMBOL",
            "name": "atom"
          }
        },
        {
          "type": "SYMBOL",
          "name": "integer"
        },
        {
          "type": "SYMBOL",
          "name": "rational"
        },
        {
          "type": "PREC",
          "value": 10,
          "content": {
            "type": "SYMBOL",
            "name": "binop_term"
          }
        },
        {
          "type": "SYMBOL",
          "name": "parenthesized_term"
        },
        {
          "type": "PREC",
          "value": 50,
          "content": {
            "type": "SYMBOL",
            "name": "quasi_quotation"
          }
        },
        {
          "type": "SYMBOL",
          "name": "curly_braced_term"
        },
        {
          "type": "SYMBOL",
          "name": "dict_literal"
        }
      ]
    },
    "compound_term": {
      "type": "PREC",
      "value": 100,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "functor",
            "content": {
              "type": "SYMBOL",
              "name": "atom"
            }
          },
          {
            "type": "STRING",
            "value": "("
          },
          {
            "type": "PREC",
            "value": 5,
            "content": {
              "type": "SYMBOL",
              "name": "_restricted_operators_term"
            }
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": ","
                },
                {
                  "type": "PREC",
                  "value": 5,
                  "content": {
                    "type": "SYMBOL",
                    "name": "_restricted_operators_term"
                  }
                }
              ]
            }
          },
          {
            "type": "STRING",
            "value": ")"
          }
        ]
      }
    },
    "atom": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "unquoted_atom"
        },
        {
          "type": "SYMBOL",
          "name": "quoted_atom"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "graphic_char_atom"
            },
            {
              "type": "STRING",
              "value": "!"
            }
          ]
        }
      ]
    },
    "graphic_char_atom": {
      "type": "PATTERN",
      "value": "[-+*/\\\\^<>=~:.?@#$&]+"
    },
    "unquoted_atom": {
      "type": "PATTERN",
      "value": "[a-z][a-zA-Z_]*"
    },
    "quoted_atom": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "'"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_single_quoted_inner"
          }
        },
        {
          "type": "PATTERN",
          "value": "'"
        }
      ]
    },
    "_single_quoted_inner": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "single_quoted_character_escape"
        },
        {
          "type": "SYMBOL",
          "name": "format_string_placeholder"
        },
        {
          "type": "SYMBOL",
          "name": "single_quoted_content"
        }
      ]
    },
    "format_string_placeholder": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "~"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "(`.)?t"
              },
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "PATTERN",
                        "value": "\\d+|\\*"
                      },
                      {
                        "type": "BLANK"
                      }
                    ]
                  },
                  {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ":"
                      },
                      {
                        "type": "BLANK"
                      }
                    ]
                  },
                  {
                    "type": "PATTERN",
                    "value": "[acd~DeEgfGiknNpqrR@s\\|\\+wW]"
                  }
                ]
              }
            ]
          }
        ]
      }
    },
    "single_quoted_character_escape": {
      "type": "PATTERN",
      "value": "\\\\[nrtv\\\\']"
    },
    "single_quoted_content": {
      "type": "PATTERN",
      "value": "[^\\\\~']+"
    },
    "string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "double_quoted_string"
        },
        {
          "type": "SYMBOL",
          "name": "backtick_string"
        }
      ]
    },
    "double_quoted_string": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\""
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_double_quoted_string_inner"
          }
        },
        {
          "type": "PATTERN",
          "value": "\""
        }
      ]
    },
    "_double_quoted_string_inner": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "format_string_placeholder"
        },
        {
          "type": "SYMBOL",
          "name": "double_quoted_character_escape"
        },
        {
          "type": "SYMBOL",
          "name": "double_quoted_content"
        }
      ]
    },
    "double_quoted_character_escape": {
      "type": "PATTERN",
      "value": "\\\\[nrtv\\\\\"]"
    },
    "double_quoted_content": {
      "type": "PATTERN",
      "value": "[^\\\\~\"]+"
    },
    "backtick_string": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "`"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_backtick_string_inner"
          }
        },
        {
          "type": "PATTERN",
          "value": "`"
        }
      ]
    },
    "_backtick_string_inner": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "format_string_placeholder"
        },
        {
          "type": "SYMBOL",
          "name": "backticked_character_escape"
        },
        {
          "type": "SYMBOL",
          "name": "backticked_content"
        }
      ]
    },
    "backticked_character_escape": {
      "type": "PATTERN",
      "value": "\\\\[nrtv\\\\`]"
    },
    "backticked_content": {
      "type": "PATTERN",
      "value": "[^\\\\~`]+"
    },
    "variable": {
      "type": "PATTERN",
      "value": "[_A-Z][a-zA-Z0-9_]*"
    },
    "list_literal": {
      "type": "PREC",
      "value": 100,
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "["
              },
              {
                "type": "STRING",
                "value": "]"
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "["
              },
              {
                "type": "PREC",
                "value": 15,
                "content": {
                  "type": "SYMBOL",
                  "name": "_restricted_operators_term"
                }
              },
              {
                "type": "PREC_LEFT",
                "value": 50,
                "content": {
                  "type": "REPEAT",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ","
                      },
                      {
                        "type": "PREC",
                        "value": 15,
                        "content": {
                          "type": "SYMBOL",
                          "name": "_restricted_operators_term"
                        }
                      }
                    ]
                  }
                }
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": "|"
                      },
                      {
                        "type": "SYMBOL",
                        "name": "_term"
                      }
                    ]
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              },
              {
                "type": "STRING",
                "value": "]"
              }
            ]
          }
        ]
      }
    },
    "integer": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "0[xX][0-9a-fA-F]+"
        },
        {
          "type": "PATTERN",
          "value": "0b[01]+"
        },
        {
          "type": "PATTERN",
          "value": "0o[0-7]+"
        },
        {
          "type": "PATTERN",
          "value": "[-+]?[1-9][0-9]*"
        },
        {
          "type": "PATTERN",
          "value": "[-+]?0"
        }
      ]
    },
    "rational": {
      "type": "PATTERN",
      "value": "-?[0-9]+r[0-9]+"
    },
    "binop_term": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "left",
            "content": {
              "type": "SYMBOL",
              "name": "_term"
            }
          },
          {
            "type": "FIELD",
            "name": "operator",
            "content": {
              "type": "SYMBOL",
              "name": "operator"
            }
          },
          {
            "type": "FIELD",
            "name": "right",
            "content": {
              "type": "SYMBOL",
              "name": "_term"
            }
          }
        ]
      }
    },
    "operator": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "non_comma_operator"
        },
        {
          "type": "STRING",
          "value": ","
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "non_comma_operator": {
      "type": "PATTERN",
      "value": "[-+*/\\\\^<>=~:.?@#$&]+"
    },
    "prefix_operator_term": {
      "type": "PREC",
      "value": 100,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "operator",
            "content": {
              "type": "SYMBOL",
              "name": "non_comma_operator"
            }
          },
          {
            "type": "FIELD",
            "name": "operand",
            "content": {
              "type": "SYMBOL",
              "name": "_term"
            }
          }
        ]
      }
    },
    "parenthesized_term": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "SYMBOL",
          "name": "_term"
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "curly_braced_term": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "SYMBOL",
          "name": "_term"
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "dict_literal": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "tag",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "atom"
              },
              {
                "type": "SYMBOL",
                "name": "variable"
              }
            ]
          }
        },
        {
          "type": "FIELD",
          "name": "keys_values",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "{"
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "SYMBOL",
                        "name": "dict_key_value_pair"
                      },
                      {
                        "type": "REPEAT",
                        "content": {
                          "type": "SEQ",
                          "members": [
                            {
                              "type": "STRING",
                              "value": ","
                            },
                            {
                              "type": "SYMBOL",
                              "name": "dict_key_value_pair"
                            }
                          ]
                        }
                      }
                    ]
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              },
              {
                "type": "STRING",
                "value": "}"
              }
            ]
          }
        }
      ]
    },
    "dict_key_value_pair": {
      "type": "PREC_LEFT",
      "value": 20,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "dict_key",
            "content": {
              "type": "SYMBOL",
              "name": "_non_graphic_char_atom"
            }
          },
          {
            "type": "STRING",
            "value": ":"
          },
          {
            "type": "FIELD",
            "name": "dict_value",
            "content": {
              "type": "SYMBOL",
              "name": "_restricted_operators_term"
            }
          }
        ]
      }
    },
    "_non_graphic_char_atom": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "unquoted_atom"
        },
        {
          "type": "SYMBOL",
          "name": "quoted_atom"
        }
      ]
    },
    "eol_comment": {
      "type": "PATTERN",
      "value": "%.*"
    },
    "quasi_quotation": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{|"
        },
        {
          "type": "FIELD",
          "name": "syntax",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "atom"
              },
              {
                "type": "SYMBOL",
                "name": "compound_term"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "||"
        },
        {
          "type": "FIELD",
          "name": "quotation",
          "content": {
            "type": "SYMBOL",
            "name": "quasi_quotation_body"
          }
        },
        {
          "type": "STRING",
          "value": "|}"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s+"
    },
    {
      "type": "SYMBOL",
      "name": "eol_comment"
    }
  ],
  "conflicts": [
    [
      "non_comma_operator",
      "graphic_char_atom"
    ]
  ],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "read_term_end_token"
    },
    {
      "type": "SYMBOL",
      "name": "quasi_quotation_body"
    }
  ],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}